    "karma-chrome-launcher": "^2.0.0",
    "karma-jasmine": "^1.0.2",
    "karma-mocha": "^1.0.1",
    "mocha": "^3.0.2",
    "multiline": "^1.0.2",
    "nightwatch": "^0.9.8",
    "nightwatch-html-reporter": "^1.0.4",
    "nodemon": "^1.8.1",
    "phantomjs-prebuilt": "^2.1.7",
    "selenium-standalone": "^5.9.0",
    "shelljs": "^0.7.6",
    "standard": "latest",
    "supertest": "^2.0.0"
  }
}
